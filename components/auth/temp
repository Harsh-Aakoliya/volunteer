// const twilio = require('twilio');
// require('dotenv').config();
import twilio from 'twilio';
import dotenv from 'dotenv';
dotenv.config();
const client = twilio(
  process.env.TWILIO_ACCOUNT_SID,
  process.env.TWILIO_AUTH_TOKEN
);
export default client;
module.exports = client;