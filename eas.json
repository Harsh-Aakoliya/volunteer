{
    "cli": {
      "version": ">= 16.15.0",
      "appVersionSource": "remote"
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal",
        "node": "20.19.4"
      },
      "preview": {
    "distribution": "internal",
    "autoIncrement": true,
    "android": {
      "buildType": "apk",
      "gradleCommand": ":app:assembleRelease",
      "image": "latest"
    },
    "node": "20.19.4",
    "env": {
      "EXPO_PUBLIC_ENVIRONMENT": "preview"
    },
    "cache": {
      "key": "preview-cache"
    }
  },
  
      "production": {
        "autoIncrement": true,
        "android": {
          "buildType": "apk",
          "gradleCommand": ":app:assembleRelease"
        },
        "env": {
          "EXPO_PUBLIC_ENVIRONMENT": "production"
        },
        "cache": {
          "key": "production-cache"
        }
      }
    },
    "submit": {
      "production": {}
    }
  }